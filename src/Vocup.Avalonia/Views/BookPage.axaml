<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:Vocup.Avalonia.ViewModels;assembly=Vocup.Avalonia"
             xmlns:c="clr-namespace:Vocup.Avalonia.Controls;assembly=Vocup.Avalonia"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Vocup.Avalonia.Views.BookPage">

  <Design.DataContext>
    <vm:BookPageVM />
  </Design.DataContext>
  
  <DockPanel>
    <c:MenuStrip DockPanel.Dock="Top" DataContext="{Binding MenuStrip}" />
    <c:ToolBar DockPanel.Dock="Left" DataContext="{Binding ToolBar}" />
  </DockPanel>
</UserControl>
